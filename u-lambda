
set -o errexit
set -v


scc 'int i=1;  (+ _1)(i)'
scc 'int i=1;  (+ + _1)(i)'
scc '(- _1)(3), (+ - - - + _1)(3)'
scc '(_1 +_2)(1,2)'
scc '(_1 +_2)(MT(1,2,3))'
scc 'range(10) || (_1 +_2)'
scc '(_1 +_1)(1)'
scc 'var(11)(1, 2)'


: '- - - - - - - - - - - - - - - - - - - - - -  U-LAMBDA passed   - -- - - - - - - - - - - - - - - - - - - - -'
